{
    "manifest_version": 2,
    "name": "Clock on Top",
    "description": "Puts a clock on top of web pages",
    "version": "1.0.0",
    "content_scripts":[
        {
            "matches":["http://*/*", "https://*/*"],
            "js":["script.js"],
            "css":["styles.css"]
        }
    ]
}